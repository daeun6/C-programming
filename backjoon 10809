#include<iostream>
#include<string>
#include<string.h>

using namespace std;

int main()
{
	string alpha = "abcdefghijklmnopqrstuvwxyz";
	string s;
	getline(cin, s, '\n');

	for (int i = 0; i < alpha.length(); i++)
	{
		if (s.find(alpha[i]) == string::npos)
		{
			cout << -1 << " ";

		}
		else {
			cout << s.find(alpha[i]) << " ";
		}
	}
}
